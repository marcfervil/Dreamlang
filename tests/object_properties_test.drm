class three{
    previous = two()
    func value(m){
        #print(self.parent.previous)
        return (previous.value(1) + 1) * m
    }
    func getPrevious(){
        return previous
    }
}

class two {
    previous = one()
    func value(m){

        return (previous.value(1) + 1) * m
    }
    func getPrevious(){
        return previous
    }
}

class one {
    func value(m){
        return 1 * m
    }
}

number = three()

check("object property call #1", 3, number.value(1))
check("object property variable to method call #2", 1, number.previous.previous.value(1))
check("nested object property call ", 10, number.getPrevious().getPrevious().value(10))
