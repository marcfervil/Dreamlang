
class hello {
   name = ""
   func init(name)

   func getGreeting() => "hello, " + name

   func setName(new_name) -> name = new_name

   func rep() => "Greeter(" + name + ")"
}

greeter = hello("marc")


print(greeter.getGreeting())

print(greeter)

greeter.setName("john")
print(greeter.getGreeting())

print(greeter)
