

class Greet {
    msg = "Generic Greeting"

    func get_msg(){
        return msg
    }

    func show(){
        print(get_msg())
    }
}

class GenericGreet extends Greet{

    func init(){
        super()
    }

}

class Hello extends Greet{

    func init(){
        super()
    }

    func get_msg(){
        return "Hello"
    }

}

greet = GenericGreet()
greet.show()

hello = Hello()
hello.show()
